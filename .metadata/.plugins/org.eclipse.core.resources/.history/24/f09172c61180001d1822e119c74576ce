package c;

public class Params {

	public static void main(String[] args) {
		
		
		sayHi();
		sayName("BERNARDO");
		sayName("FERNANDO");
		sayName("ALFREDO");
		sayName("LEONARDO");
		
	}
	public static void sayHi() {
		System.out.println("hi");
	}
	
	public static void sayName(String name) {
		
		System.out.println(name);
	}

}
